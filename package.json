{
  "name": "theme-next.js",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npx @tailwindcss/cli -i ./themes/customerA.css -o ./public/themes/customerA.css --watch & npx @tailwindcss/cli -i ./themes/customerB.css -o ./public/themes/customerB.css --watch & next dev --turbopack",
    "build": "npx @tailwindcss/cli -i ./themes/customerA.css -o ./public/themes/customerA.css --minify & npx @tailwindcss/cli -i ./themes/customerB.css -o ./public/themes/customerB.css --minify & next build --turbopack",
    "start": "next start",
    "lint": "eslint"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.540.0",
    "next": "15.5.9",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5"
  }
}
